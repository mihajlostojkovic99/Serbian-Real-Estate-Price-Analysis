{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "outDir": "dist",
        "noUnusedLocals": false,
        "lib": ["DOM"],
        "baseUrl": "src",
        "esModuleInterop": true,
        "moduleResolution": "nodenext",
        "emitDeclarationOnly": true,
        "allowImportingTsExtensions": true,
    },
    "include": [
        "./src/**/*"
    ]
}
